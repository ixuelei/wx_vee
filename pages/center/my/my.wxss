/* pages/center/my/my.wxss */

page {
  background-color: #f0f0f0;
}

.top {
  height: 213rpx;
  background-image: url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-size: 100% 100%;
}

.top {
  align-items: center;
  display: flex;
  padding: 0 20rpx;
  position: relative;
}

.top .portrait {
  width: 165rpx;
  height: 165rpx;
  border-radius: 50%;
  margin-left: 20rpx;
}

.top view {
  margin-left: 20rpx;
}

.top view text {
  display: block;
  color: #fff;
}

.top view text:nth-child(1) {
  font-size: 45rpx;
}

.top view text:nth-child(2) {
  font-size: 38rpx;
}

.top .down {
  position: absolute;
  right: 40rpx;
  top: 95rpx;
  width: 25rpx;
  height: 25rpx;
  border-color: #fff;
  border-width: 5rpx 0 0 5rpx;
  border-style: solid;
  transform: rotate(135deg);
}

/* 钱包 */

.wallet {
  width: 96%;
  margin: 20rpx 2%;
  background: #fff;
  padding: 20rpx 0;
  border-radius: 10rpx;
}

.wallet .wallet-1 {
  position: relative;
}

.wallet .wallet-1-1 {
  display: flex;
  align-items: center;
  padding: 0 20rpx;
}

.wallet .wallet-1-1 .h2 {
  color: #333;
  font-size: 38rpx;
}

.wallet .wallet-1-1 .p {
  color: #333;
  font-size: 30rpx;
  margin-left: 15rpx;
}

.wallet .walleImg {
  width: 70rpx;
  height: 70rpx;
}

.wallet .wallet-1-1 .go {
  display: flex;
  position: absolute;
  right: 20rpx;
  top: 15rpx;
  font-size: 32rpx;
  color: #999;
}

.wallet .down {
  margin-top: 13rpx;
  margin-right: 10rpx;
  width: 20rpx;
  height: 20rpx;
  border-color: #999;
  border-width: 4rpx 0 0 4rpx;
  border-style: solid;
  transform: rotate(135deg);
}
.wallet .wallet-1-2{
  margin-left: 100rpx;
  color: #999;
  font-size: 30rpx;
}
.wallet .wallet-1-2 text text{
  display:inline-block;
  border-radius: 50%;
  width: 10rpx;
  height: 10rpx;
  margin:0 15rpx;
  background: #999;
}
.wallet .line{
  width: 90%;
  margin: 20rpx auto;
  border-bottom: 4rpx solid #EAEAEA;
}

.wallet .wallet-2{
  display: flex;
  align-items: center;
  height: 70rpx;
  line-height: 70rpx;
  padding: 0 20rpx;
  font-size: 34rpx;
}
.wallet .wallet-2 view{
  display: flex;
  width: 50%;
}
.wallet .wallet-2 view:nth-child(2){
  position: relative;
}
.wallet .wallet-2 view:nth-child(1) view:nth-child(2){
  color: #4A84FE;
  font-size: 30rpx;
  flex: 1;
}
.wallet .wallet-2 view:nth-child(1) view:nth-child(2) text{
  display: inline-block;
  width: 100%;
  margin-right: 15rpx;
  text-align: right;
}
.wallet .wallet-2 view view:nth-child(3){
  color: #4A84FE;
  font-size: 30rpx;
  flex: 1;
}
.wallet .wallet-2 view view:nth-child(3) text{
  display: inline-block;
  width: 100%;

  text-align: right;
}
.wallet .wallet-2 .left-line{
  position: absolute;
  left: -2rpx;
  top: 5rpx;
  width: 4rpx;
  background: #EAEAEA;
  height: 60rpx;
}

/* 订单 */
.order {
  width: 96%;
  margin: 20rpx 2%;
  background: #fff;
  padding: 20rpx 0;
  border-radius: 10rpx;
}
.order .h2{
  font-size: 38rpx;
  margin-left:20rpx;
}
.order .placeholder{
  margin-top: 20rpx;
  text-align: center;
}
.order .placeholder image{
  width: 100rpx;
  height: 100rpx;
}
.order .placeholder text{
   display: inline-block;
   font-size: 30rpx;
   color: #333;
}
